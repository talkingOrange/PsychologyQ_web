<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>ReviewPost</servlet-name>
  	<display-name>ReviewPost</display-name>
  	<description></description>
  	<servlet-class>review.ReviewPost</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>servletPage.LoginServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>ScehduleSaveServlet</servlet-name>
  	<display-name>ScehduleSaveServlet</display-name>
  	<description></description>
  	<servlet-class>servletPage.ScehduleSaveServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ReviewsServlet</servlet-name>
  	<display-name>ReviewsServlet</display-name>
  	<description></description>
  	<servlet-class>servletPage.ReviewsServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CounselsServlet</servlet-name>
  	<display-name>CounselsServlet</display-name>
  	<description></description>
  	<servlet-class>servletPage.CounselsServlet</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>ScheduleGetServlet</servlet-name>
  	<display-name>ScheduleGetServlet</display-name>
  	<description></description>
  	<servlet-class>servletPage.ScheduleGetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ReviewPost</servlet-name>
  	<url-pattern>/review</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ScehduleSaveServlet</servlet-name>
  	<url-pattern>/scheduleSave</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ReviewsServlet</servlet-name>
  	<url-pattern>/reviews</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CounselsServlet</servlet-name>
  	<url-pattern>/counsels</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ScheduleGetServlet</servlet-name>
  	<url-pattern>/scheduleGet</url-pattern>
  </servlet-mapping>
</web-app>
